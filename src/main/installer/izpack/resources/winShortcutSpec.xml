<shortcuts>
  <skipIfNotSupported/>

  <programGroup defaultName="$appGroup"
                location="applications"/>

  <shortcut
    name="VentPlan"
    target="$INSTALL_PATH\bin\VentPlan.bat"
    commandLine=""
    workingDirectory="$INSTALL_PATH\bin"
    description="Run VentPlan"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    iconFile="$INSTALL_PATH\icons\griffon-logo.png"
    applications="yes"
    startMenu="no"
    startup="no">
  </shortcut>

  <shortcut
    name="Uninstall"
    target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
    commandLine=""
    workingDirectory="$INSTALL_PATH"
    description="Uninstall VentPlan"
    initialState="normal"
    programGroup="yes"
    desktop="no"
    applications="no"
    startMenu="no"
    startup="no">
  </shortcut>

</shortcuts>