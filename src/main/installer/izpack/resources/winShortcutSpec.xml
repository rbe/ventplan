<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="$appGroup"
                  location="applications"/>
    <shortcut
            name="Ventplan"
            target="$INSTALL_PATH\bin\ventplan.bat"
            commandLine=""
            workingDirectory="$INSTALL_PATH\bin"
            description="Run Ventplan"
            initialState="normal"
            programGroup="yes"
            desktop="yes"
            iconFile="$INSTALL_PATH\icons\ventplan_signet_16x16.png"
            applications="yes"
            startMenu="no"
            startup="no">
    </shortcut>
    <shortcut
            name="Uninstall"
            target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
            commandLine=""
            workingDirectory="$INSTALL_PATH"
            description="Uninstall Ventplan"
            initialState="normal"
            programGroup="yes"
            desktop="no"
            applications="no"
            startMenu="no"
            startup="no">
    </shortcut>
</shortcuts>
