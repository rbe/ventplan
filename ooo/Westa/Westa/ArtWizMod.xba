option explicit

&apos;!! ============================
&apos;!! Event: OK im dialog geklickt
&apos;!! ============================
sub pbOKSelect(e)

	_Global.copyEntry(false, array( _
			oDummyDlg.getControl(&quot;etBeschreibung&quot;).Text, _
			oDummyDlg.getControl(&quot;etEinzelpreis&quot;).Text, _
			oDummyDlg.getControl(&quot;etLM&quot;).Text, _
			oDummyDlg.getControl(&quot;etME&quot;).Text, _
			oDummyDlg.getControl(&quot;etVE&quot;).Text, _
			oDummyDlg.getControl(&quot;etAnzahl&quot;).Text, _
	oDummyDlg.endExecute()
end sub

sub etMECharinput(e)

	e.Source.Context.getControl(&quot;stME&quot;).setText(e.Source.Text)
end sub

sub etEinzelpreisModified(e)
	e.Source.Context.getControl(&quot;pbOK&quot;).setEnable(isNumeric(e.Source.Text))
end sub
</script:module>